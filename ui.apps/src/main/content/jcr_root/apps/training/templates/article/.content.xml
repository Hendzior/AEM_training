            sling:resourceType="training/components/structure/logo"/>
        <title
            jcr:primaryType="nt:unstructured"
            sling:resourceType="training/components/content/title"/>
        <topnav
            jcr:primaryType="nt:unstructured"
            sling:resourceType="training/components/structure/topnav"/>
        <image
                jcr:primaryType="nt:unstructured"
                sling:resourceType="training/components/content/image"/>
        <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="training/components/content/text"
                text="&lt;p>This is a article page&lt;/p>"
